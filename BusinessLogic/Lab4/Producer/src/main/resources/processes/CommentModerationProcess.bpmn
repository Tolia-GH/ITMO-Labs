      <bpmndi:BPMNShape id="Task_PersistComment_di" bpmnElement="Task_PersistComment">
        <dc:Bounds x="300" y="280" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_CreateExternalTask_di" bpmnElement="Task_CreateExternalTask">
        <dc:Bounds x="450" y="280" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="UserTask_ReviewComment_di" bpmnElement="UserTask_ReviewComment">
        <dc:Bounds x="600" y="470" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_ReviewDecision_di" bpmnElement="Gateway_ReviewDecision" isMarkerVisible="true">
        <dc:Bounds x="765" y="295" width="50" height="50" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="764" y="271" width="53" height="14" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_MarkApproved_di" bpmnElement="Task_MarkApproved">
        <dc:Bounds x="880" y="230" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Task_MarkRejected_di" bpmnElement="Task_MarkRejected">
        <dc:Bounds x="880" y="340" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Gateway_Join_di" bpmnElement="Gateway_Join" isMarkerVisible="true">
        <dc:Bounds x="1035" y="295" width="50" height="50" />
      </bpmndi:BPMNShape>
