        <dc:Bounds x="1150" y="280" width="100" height="80" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_CommentProcessed_di" bpmnElement="EndEvent_CommentProcessed">
        <dc:Bounds x="1322" y="302" width="36" height="36" />
        <bpmndi:BPMNLabel>
          <dc:Bounds x="1315" y="345" width="51" height="27" />
        </bpmndi:BPMNLabel>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_Start_To_WriteComment_di" bpmnElement="Flow_Start_To_WriteComment">
        <di:waypoint x="248" y="140" />
