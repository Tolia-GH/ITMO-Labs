<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL" xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI" xmlns:dc="http://www.omg.org/spec/DD/20100524/DC" xmlns:camunda="http://camunda.org/schema/1.0/bpmn" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:di="http://www.omg.org/spec/DD/20100524/DI" xmlns:modeler="http://camunda.org/schema/modeler/1.0" id="Definitions_CommentModeration" targetNamespace="http://bpmn.io/schema/bpmn" exporter="Camunda Modeler" exporterVersion="5.42.0" modeler:executionPlatform="Camunda Platform" modeler:executionPlatformVersion="7.17.0">
  <bpmn:collaboration id="Collaboration_CommentModeration">
    <bpmn:participant id="Participant_CommentProcess" name="Comment Moderation Process" processRef="CommentModerationProcess" />
  </bpmn:collaboration>
  <bpmn:process id="CommentModerationProcess" name="Comment Moderation Process" isExecutable="true" camunda:historyTimeToLive="180">
    <bpmn:laneSet id="LaneSet_Roles">
      <bpmn:lane id="Lane_User" name="User">
        <bpmn:flowNodeRef>StartEvent_SubmitComment</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_System" name="System">
        <bpmn:flowNodeRef>Task_PersistComment</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CreateExternalTask</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_ReviewDecision</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_MarkApproved</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_MarkRejected</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Gateway_Join</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>Task_CompleteExternal</bpmn:flowNodeRef>
        <bpmn:flowNodeRef>EndEvent_CommentProcessed</bpmn:flowNodeRef>
      </bpmn:lane>
      <bpmn:lane id="Lane_Admin" name="Admin">
        <bpmn:flowNodeRef>UserTask_ReviewComment</bpmn:flowNodeRef>
      </bpmn:lane>
    </bpmn:laneSet>
    <bpmn:startEvent id="StartEvent_SubmitComment" name="User starts process" camunda:initiator="userEmail">
      <bpmn:outgoing>Flow_Start_To_WriteComment</bpmn:outgoing>
    </bpmn:startEvent>
    <bpmn:userTask id="UserTask_WriteComment" name="Write comment" camunda:formRef="comment-submission" camunda:formRefBinding="latest" camunda:candidateGroups="USER">
      <bpmn:extensionElements>
        <camunda:taskListener delegateExpression="${userEmailInjectorListener}" event="create" />
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Start_To_WriteComment</bpmn:incoming>
      <bpmn:outgoing>Flow_WriteComment_To_Persist</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:serviceTask id="Task_PersistComment" name="Persist comment (Under Review)" camunda:delegateExpression="${persistCommentDelegate}">
      <bpmn:incoming>Flow_WriteComment_To_Persist</bpmn:incoming>
      <bpmn:outgoing>Flow_Persist_To_External</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_CreateExternalTask" name="Create task in external system" camunda:delegateExpression="${createExternalTaskDelegate}">
      <bpmn:incoming>Flow_Persist_To_External</bpmn:incoming>
      <bpmn:outgoing>Flow_External_To_Review</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:userTask id="UserTask_ReviewComment" name="Review comment" camunda:formRef="comment-review" camunda:formRefBinding="latest" camunda:candidateGroups="ADMIN">
      <bpmn:extensionElements />
      <bpmn:incoming>Flow_External_To_Review</bpmn:incoming>
      <bpmn:outgoing>Flow_Review_To_Gateway</bpmn:outgoing>
    </bpmn:userTask>
    <bpmn:exclusiveGateway id="Gateway_ReviewDecision" name="Approved?">
      <bpmn:incoming>Flow_Review_To_Gateway</bpmn:incoming>
      <bpmn:outgoing>Flow_Approved</bpmn:outgoing>
      <bpmn:outgoing>Flow_Rejected</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_MarkApproved" name="Mark as APPROVED" camunda:delegateExpression="${updateCommentStatusDelegate}">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="status">APPROVED</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Approved</bpmn:incoming>
      <bpmn:outgoing>Flow_Approved_To_Join</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:serviceTask id="Task_MarkRejected" name="Mark as REJECTED" camunda:delegateExpression="${updateCommentStatusDelegate}">
      <bpmn:extensionElements>
        <camunda:inputOutput>
          <camunda:inputParameter name="status">REJECTED</camunda:inputParameter>
        </camunda:inputOutput>
      </bpmn:extensionElements>
      <bpmn:incoming>Flow_Rejected</bpmn:incoming>
      <bpmn:outgoing>Flow_Rejected_To_Join</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:exclusiveGateway id="Gateway_Join">
      <bpmn:incoming>Flow_Approved_To_Join</bpmn:incoming>
      <bpmn:incoming>Flow_Rejected_To_Join</bpmn:incoming>
      <bpmn:outgoing>Flow_Join_To_CompleteExternal</bpmn:outgoing>
    </bpmn:exclusiveGateway>
    <bpmn:serviceTask id="Task_CompleteExternal" name="Complete external task" camunda:delegateExpression="${completeExternalTaskDelegate}">
      <bpmn:incoming>Flow_Join_To_CompleteExternal</bpmn:incoming>
      <bpmn:outgoing>Flow_CompleteExternal_To_End</bpmn:outgoing>
    </bpmn:serviceTask>
    <bpmn:endEvent id="EndEvent_CommentProcessed" name="Comment processed">
      <bpmn:incoming>Flow_CompleteExternal_To_End</bpmn:incoming>
    </bpmn:endEvent>
    <bpmn:sequenceFlow id="Flow_Start_To_WriteComment" sourceRef="StartEvent_SubmitComment" targetRef="UserTask_WriteComment" />
    <bpmn:sequenceFlow id="Flow_WriteComment_To_Persist" sourceRef="UserTask_WriteComment" targetRef="Task_PersistComment" />
    <bpmn:sequenceFlow id="Flow_Persist_To_External" sourceRef="Task_PersistComment" targetRef="Task_CreateExternalTask" />
    <bpmn:sequenceFlow id="Flow_External_To_Review" sourceRef="Task_CreateExternalTask" targetRef="UserTask_ReviewComment" />
    <bpmn:sequenceFlow id="Flow_Review_To_Gateway" sourceRef="UserTask_ReviewComment" targetRef="Gateway_ReviewDecision" />
    <bpmn:sequenceFlow id="Flow_Approved" name="Yes" sourceRef="Gateway_ReviewDecision" targetRef="Task_MarkApproved">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${status == 'APPROVED'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Rejected" name="No" sourceRef="Gateway_ReviewDecision" targetRef="Task_MarkRejected">
      <bpmn:conditionExpression xsi:type="bpmn:tFormalExpression">${status == 'REJECTED'}</bpmn:conditionExpression>
    </bpmn:sequenceFlow>
    <bpmn:sequenceFlow id="Flow_Approved_To_Join" sourceRef="Task_MarkApproved" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_Rejected_To_Join" sourceRef="Task_MarkRejected" targetRef="Gateway_Join" />
    <bpmn:sequenceFlow id="Flow_Join_To_CompleteExternal" sourceRef="Gateway_Join" targetRef="Task_CompleteExternal" />
    <bpmn:sequenceFlow id="Flow_CompleteExternal_To_End" sourceRef="Task_CompleteExternal" targetRef="EndEvent_CommentProcessed" />
  </bpmn:process>
  <bpmndi:BPMNDiagram id="BPMNDiagram_CommentModeration">
    <bpmndi:BPMNPlane id="BPMNPlane_CommentModeration" bpmnElement="Collaboration_CommentModeration">
      <bpmndi:BPMNShape id="Participant_CommentProcess_di" bpmnElement="Participant_CommentProcess" isHorizontal="true">
        <dc:Bounds x="120" y="80" width="1300" height="490" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_Admin_di" bpmnElement="Lane_Admin" isHorizontal="true">
        <dc:Bounds x="150" y="450" width="1270" height="120" />
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="Lane_System_di" bpmnElement="Lane_System" isHorizontal="true">
        <dc:Bounds x="150" y="200" width="1270" height="250" />
