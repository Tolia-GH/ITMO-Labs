      <bpmndi:BPMNEdge id="Flow_Rejected_To_Join_di" bpmnElement="Flow_Rejected_To_Join">
        <di:waypoint x="980" y="380" />
        <di:waypoint x="1000" y="380" />
        <di:waypoint x="1000" y="320" />
        <di:waypoint x="1035" y="320" />
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Join_To_CompleteExternal_di" bpmnElement="Flow_Join_To_CompleteExternal">
        <di:waypoint x="1085" y="320" />
        <di:waypoint x="1150" y="320" />
